                    </div>
                    <div class="search-bar-inner-two">
                        <input id="searchBar" type="text" placeholder="Search..." name="name" value="" />
                    </div>
                </div>
            </div>
            <div class="container-fluid">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
